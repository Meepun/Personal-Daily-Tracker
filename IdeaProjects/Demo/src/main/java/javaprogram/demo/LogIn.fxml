      <Button fx:id="mainmenuButton" layoutX="262.0" layoutY="361.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="226.0" style="-fx-background-color: #80c8b6;" text="Main Menu" onAction="#handleMainMenuButton" />

   </children>
</AnchorPane>
