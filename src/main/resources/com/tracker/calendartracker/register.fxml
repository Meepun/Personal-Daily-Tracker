<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="384.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tracker.calendartracker.SignupController">
   <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #fbaa18;">
      <ImageView fx:id="hiLogoNoTextImageView" fitHeight="69.0" fitWidth="96.0" layoutX="14.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true" />
      <Label layoutX="94.0" layoutY="31.0" text="User Sign-Up" textFill="#000000f2">
         <font>
            <Font name="Berlin Sans FB" size="20.0" />
         </font>
      </Label>
   </AnchorPane>
   <Label layoutX="92.0" layoutY="104.0" prefHeight="21.0" prefWidth="62.0" text="Username:">
      <font>
         <Font name="Berlin Sans FB Demi Bold" size="12.0" />
      </font>
   </Label>
   <TextField fx:id="emailTextField" layoutX="92.0" layoutY="125.0" prefHeight="25.0" prefWidth="336.0" promptText="Type a username..." />
   <Label layoutX="92.0" layoutY="166.0" prefHeight="21.0" prefWidth="54.0" text="Password:">
      <font>
         <Font name="Berlin Sans FB Demi Bold" size="12.0" />
      </font>
   </Label>
   <TextField fx:id="passTextField" layoutX="92.0" layoutY="188.0" prefHeight="25.0" prefWidth="336.0" promptText="Type your password..." />
   <Label layoutX="92.0" layoutY="228.0" prefHeight="21.0" prefWidth="130.0" text="Confirm password:">
      <font>
         <Font name="Berlin Sans FB Demi Bold" size="12.0" />
      </font>
   </Label>
   <TextField fx:id="conpassTextField" layoutX="92.0" layoutY="249.0" prefHeight="25.0" prefWidth="336.0" promptText="Confirm your password" />
   <Button fx:id="signupButton" layoutX="93.0" layoutY="294.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="336.0" style="-fx-background-color: #fbaa18;" text="Sign up" onAction="#handleSignupButton">
      <font>
         <Font name="Berlin Sans FB Demi Bold" size="13.0" />
      </font>
   </Button>
   <Button fx:id="mainmenuButton" layoutX="93.0" layoutY="331.0" mnemonicParsing="false" onAction="#handleMainMenuButton" prefHeight="25.0" prefWidth="336.0" style="-fx-background-color: #ffc725;" text="Return" textFill="WHITE">
      <font>
         <Font name="Berlin Sans FB Demi Bold" size="13.0" />
      </font>
   </Button>
</AnchorPane>
